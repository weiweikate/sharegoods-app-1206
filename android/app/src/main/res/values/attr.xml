<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WaveLoadingView">
        <attr name="wlv_borderWidth" format="dimension" />
        <attr name="wlv_borderColor" format="color" />
        <attr name="wlv_progressValue" format="integer" />
        <attr name="wlv_shapeType" format="enum">
            <enum name="triangle" value="0" />
            <enum name="circle" value="1" />
            <enum name="square" value="2" />
            <enum name="rectangle" value="3" />
        </attr>
        <attr name="wlv_triangle_direction" format="enum">
            <enum name="north" value="0" />
            <enum name="south" value="1" />
            <enum name="east" value="2" />
            <enum name="west" value="3" />
        </attr>
        <attr name="wlv_round_rectangle" format="boolean" />
        <attr name="wlv_round_rectangle_x_and_y" format="integer" />
        <attr name="wlv_waveColor" format="color" />
        <attr name="wlv_wave_background_Color" format="color" />
        <attr name="wlv_waveAmplitude" format="float" />
        <attr name="wlv_titleTop" format="string" />
        <attr name="wlv_titleCenter" format="string" />
        <attr name="wlv_titleBottom" format="string" />
        <attr name="wlv_titleTopSize" format="dimension" />
        <attr name="wlv_titleCenterSize" format="dimension" />
        <attr name="wlv_titleBottomSize" format="dimension" />
        <attr name="wlv_titleTopColor" format="color" />
        <attr name="wlv_titleCenterColor" format="color" />
        <attr name="wlv_titleBottomColor" format="color" />
        <attr name="wlv_titleTopStrokeColor" format="color" />
        <attr name="wlv_titleTopStrokeWidth" format="dimension" />
        <attr name="wlv_titleBottomStrokeColor" format="color" />
        <attr name="wlv_titleBottomStrokeWidth" format="dimension" />
        <attr name="wlv_titleCenterStrokeColor" format="color" />
        <attr name="wlv_titleCenterStrokeWidth" format="dimension" />
    </declare-styleable>
    <declare-styleable name="NineGridView">
        <attr name="ngv_gridSpacing" format="dimension" />
    </declare-styleable>

    <declare-styleable name="AlivcRecordUIStyle">
        <attr name="recordMusicImage" format="reference"/>
        <attr name="lightImageOpen" format="reference"/>
        <attr name="lightImageAuto" format="reference"/>
        <attr name="lightImageClose" format="reference"/>
        <attr name="lightImageUnable" format="reference"/>
        <attr name="countdownImage" format="reference"/>
        <attr name="switchCameraImage" format="reference"/>
        <attr name="finishImageUnable" format="reference"/>
        <attr name="finishImageAble" format="reference"/>
        <attr name="faceImage" format="reference"/>
        <attr name="magicImage" format="reference"/>
        <attr name="videoShootImageNormal" format="reference"/>
        <attr name="videoShootImageShooting" format="reference"/>
        <attr name="videoShootImageLongPressing" format="reference"/>
        <attr name="deleteImage" format="reference"/>
        <attr name="dotImage" format="reference"/>
        <attr name="triangleImage" format="reference"/>
    </declare-styleable>

    <declare-styleable name="AlivcEditUIStyle">
        <attr name="filterImage" format="reference"/>
        <attr name="musicImage" format="reference"/>
        <attr name="pasterImage" format="reference"/>
        <attr name="captionImage" format="reference"/>
        <attr name="mvImage" format="reference"/>
        <attr name="sound" format="reference"/>
        <attr name="effectImage" format="reference"/>
        <attr name="timeImage" format="reference"/>
        <attr name="translationImage" format="reference"/>
        <attr name="paintImage" format="reference"/>
        <attr name="playImage" format="reference"/>
        <attr name="pauseImage" format="reference"/>
        <attr name="finishImage" format="reference"/>
        <attr name="coverImage" format="reference"/>
    </declare-styleable>
</resources>