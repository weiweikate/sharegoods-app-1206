<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/ysf_iv_title_bar_right_btn"
        android:layout_width="@dimen/ysf_title_bar_icon_size"
        android:layout_height="@dimen/ysf_title_bar_icon_size"
        android:contentDescription="@null"
        android:scaleType="centerInside"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/ysf_evaluation_star_level_list_dark" />

    <TextView
        android:id="@+id/ysf_tv_title_bar_right_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ysf_enter_store"
        android:gravity="center"
        android:layout_gravity="center_horizontal"
        android:singleLine="true"
        android:textColor="@color/ysf_title_bar_text_color_dark_selector"
        android:textSize="@dimen/ysf_text_size_11" />
</LinearLayout>